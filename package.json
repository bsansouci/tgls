{
    "name": "tgls-actual", 
    "version": "0.8.4",
    "dependencies": {
        "@opam/topkg": "*",
        "@opam/ctypes-foreign": "*",
        "@opam/ocamlfind": "*",
        "@opam/ctypes": ">= 0.4.0",
        "tsdl-actual": "bsansouci/tsdl.git#esy",
        "substs": "yunxing/substs.git",
        "@opam/ocamlbuild": "*",
        "@opam/result": "*",
        "opam-installer-bin": "yunxing/opam-installer-bin.git"
    },
    "esy": {
      "build": ["ocaml pkg/pkg.ml build --pinned false && (opam-installer --prefix=$opam_prefix || true)"],
      "exportedEnvVars": {
        "tgls_installed": {
            "global": true,
            "globalCollisionBehavior": "clobber",
            "val": "true"
        },
        "tgls_enable": {
            "global": true,
            "globalCollisionBehavior": "clobber",
            "val": "enable"
        },
        "tgls_version": {
            "global": true,
            "globalCollisionBehavior": "clobber",
            "val": "0.8.4"
        },
        "FINDLIB": {
            "global": true,
            "resolveAsRelativePath": true,
            "globalCollisionBehavior": "joinPath",
            "val": "./_build/ocamlfind/lib"
        },
        "version": {
            "global": true,
            "globalCollisionBehavior": "clobber",
            "val": "0.8.4"
        },
        "PATH": {
            "global": true,
            "resolveAsRelativePath": true,
            "globalCollisionBehavior": "joinPath",
            "val": "./_build/ocamlfind/bin"
        }
      }
    },
    "debDependencies": {
      "ocaml": "esy-ocaml/ocaml#esy"
    }
}
